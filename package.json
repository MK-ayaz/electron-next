{
  "name": "my-app",
  "productName": "my workstation",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": "src/main.js",
  "scripts": {
    "dev": "concurrently \"npx vite\" \"wait-on http://localhost:5173 && cross-env NODE_ENV=development electron .\"",
    "build": "npx vite build",
    "start": "cross-env NODE_ENV=production electron .",
    "lint": "echo no-lint-configured",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "debug:main": "cross-env NODE_ENV=development ELECTRON_ENABLE_LOGGING=1 electron --inspect=9229 .",
    "debug:vite": "cross-env NODE_OPTIONS=--inspect npx vite"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.6.0",
    "@electron-forge/maker-deb": "^7.6.0",
    "@electron-forge/maker-rpm": "^7.6.0",
    "@electron-forge/maker-squirrel": "^7.6.0",
    "@electron-forge/maker-zip": "^7.6.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.6.0",
    "@electron-forge/plugin-fuses": "^7.6.0",
    "@electron/fuses": "^1.8.0",
    "@types/node": "^24.5.2",
    "@vitejs/plugin-react": "^5.0.3",
    "concurrently": "^9.1.0",
    "cross-env": "^7.0.3",
    "electron": "33.2.1",
    "node-loader": "^2.1.0",
    "vite": "^7.1.7",
    "wait-on": "^8.0.1"
  },
  "keywords": [],
  "author": "PC",
  "license": "MIT",
  "dependencies": {
    "electron-squirrel-startup": "^1.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
